.report_accordion {
	list-style-type: none;
	padding: 0;
	margin: 0 0 30px;
	border-top: none;
	border-left: none;
}
.report_accordion ul {
	padding: 0;
	margin: 0;
	float: left;
	display: block;
	width: 100%;
}
.report_accordion li {
	background: #ddc8e5;
	cursor: pointer;
	list-style-type: none;
	padding: 0;
	float: left;
	display: block;
	width: 100%;
}
.report_accordion > li {
	margin-bottom: 2px;
	border-radius: 4px;
}
.report_accordion li div {
	padding: 20px;
	background: #aef;
	display: block;
	clear: both;
	float: left;
	width: 360px;
}
.report_accordion a {
	text-decoration: none;
	font-weight: bold;
	font-size: 13px;
	color: #222;
	padding: 8px 15px;
	display: block;
	cursor: pointer;
}


.report_accordion .inner li a {
	background: none
}
.report_accordion .inner li a:hover {
	background-color: #E7ECFA;
}
.report_accordion .inner li.innermost a {
	padding: 8px 20px 8px 15px;
	display: block;
}

.report_accordion li ul li {
	background: #c5b0cd;
	font-size: 0.9em;
}
.report_accordion li ul li a {
	font-weight: normal;
}
.report_accordion li ul li ul li a {
	font-size: 12px;
	background: #eaf0ff !important;
	padding-left: 25px;
}
.report_accordion li ul li ul li a:hover {
	background: #F4F7FE !important;
}
.report_accordion .innermost .inner a {
	padding-left: 25px !important;
}
.report_accordion li ul li a:hover{background:#4f3a57; color:#fff;}
.report_accordion li ul li.active{background-color:#00abe8; color:#fff;}
.report_accordion a.active{background-color:#00abe8; color:#fff;}

